{
  "plugins": ["stylelint-prettier"],
  "extends": ["stylelint-config-standard"],
  "overrides": [
    {
      "files": ["*.less", "**/*.less"],
      "customSyntax": "postcss-less"
    }
  ],
  "rules": {
    "prettier/prettier": true,
    "no-empty-source": null,
    "no-descending-specificity": null
  }
}
